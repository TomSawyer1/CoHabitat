{
  "expo": {
    "name": "GuardConnect",
    "slug": "GuardConnect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "guardconnect",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.guardconnect.app",
      "associatedDomains": ["applinks:guardconnect.com"]
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "_internal": {
      "runtimeVersion": "exposdk:50.0.0"
    },
    "runtimeVersion": "50.0.0"
  }
}
