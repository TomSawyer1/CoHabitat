{
  "expo": {
    "name": "CoHabitat",
    "slug": "CoHabitat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
          "scheme": "cohabitat",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
              "bundleIdentifier": "com.cohabitat.app",
        "associatedDomains": ["applinks:cohabitat.com"]
    },
    "android": {
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "_internal": {
      "runtimeVersion": "exposdk:50.0.0"
    },
    "runtimeVersion": "50.0.0"
  }
}
